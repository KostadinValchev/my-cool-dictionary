<table class="table table-{{color}}">
    <thead class="thead-light">
        {{#each titles}}
        <th>{{this}}</th>
        {{/each}}
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            <td>{{this.contextWord}}</td>
            <td>{{this.score}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>